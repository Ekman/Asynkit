{
  "name": "@nekm/asynkit",
  "version": "1.0.0",
  "description": "Asynkit extends the capabilities of AsyncIterable in JavaScript, providing utility methods such as map, filter, and includes that are natively available for arrays but absent for async iterables.",
  "license": "MIT",
	"source": "./src/index.ts",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.esm.js",
		"types": "./dist/index.d.ts",
		"default": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.esm.js",
	"unpkg": "./dist/index.umd.js",
	"scripts": {
    "build": "microbundle --format cjs,esm,umd",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "example": "node example/example.js"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.0",
    "jest-junit": "^16.0.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "maintainers": [
    {
      "email": "niklas@niklasekman.solutions",
      "name": "Niklas Ekman",
      "url": "https://www.niklasekman.solutions"
    }
  ],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Ekman/Asynkit.git"
	},
  "homepage": "https://github.com/Ekman/Asynkit#readme"
}
